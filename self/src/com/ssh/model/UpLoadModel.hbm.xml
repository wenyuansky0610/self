<?xml version="1.0" encoding="UTF-8"?>  
	<!DOCTYPE hibernate-mapping PUBLIC  
	        '-//Hibernate/Hibernate Mapping DTD 3.0//EN'  
	        'http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd'>  
	<hibernate-mapping package="com.ssh.model">  
	    <class name="UpLoadModel" table="uploads">  
	        <id name="id" column="id">
	            <generator class="uuid"/>  <!-- uuidï¼Œnative ,assigned -->
	        </id> 
	        <property name="fileName" column="fileName" type="string" length="150"  />
            <property name="path" column="path" type="string" length="250" />
            <property name="upLoadTime" column="uploadtime" />
            <property name="fileType" column="fileType" />
            <property name="url" column="url" />
             <property name="postId" column="postId" />
	    </class>
	</hibernate-mapping>